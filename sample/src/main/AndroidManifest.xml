<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.xiaoman.library.android.rxandroidlive.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="cn.xiaoman.library.android.rxandroidlive.sample.ObservableNotRestartActivity" />
        <activity android:name="cn.xiaoman.library.android.rxandroidlive.sample.ObservableAutoRestartActivity" />
        <activity android:name="cn.xiaoman.library.android.rxandroidlive.sample.FlowableNotRestartActivity" />
        <activity android:name="cn.xiaoman.library.android.rxandroidlive.sample.FlowableAutoRestartActivity" />
        <activity android:name="cn.xiaoman.library.android.rxandroidlive.sample.SingleActivity" />
        <activity android:name="cn.xiaoman.library.android.rxandroidlive.sample.MaybeActivity" />
        <activity android:name="cn.xiaoman.library.android.rxandroidlive.sample.CompleteActivity" />

    </application>

</manifest>